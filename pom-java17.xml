<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">

	<modelVersion>4.0.0</modelVersion>

	<parent>
		<groupId>br.com.objectos</groupId>
		<artifactId>objectos</artifactId>
		<version>0.1.0-jdk17-SNAPSHOT</version>
		<relativePath>../br.com.objectos.objectos/pom-java17.xml</relativePath>
	</parent>

	<groupId>br.com.objectos</groupId>
	<artifactId>incubator</artifactId>
	<packaging>pom</packaging>

	<name>${project.groupId}:${project.artifactId}</name>

	<inceptionYear>2022</inceptionYear>

	<description>
	Under development libraries.
	</description>

	<url>https://github.com/objectos/incubator/tree/master</url>

	<licenses>
		<license>
			<name>The Apache License, Version 2.0</name>
			<url>https://www.apache.org/licenses/LICENSE-2.0</url>
			<distribution>repo</distribution>
		</license>
	</licenses>

	<scm>
		<connection>scm:git:ssh://git@rio/srv/git/incubator.git</connection>
		<developerConnection>scm:git:ssh://git@rio/srv/git/incubator.git</developerConnection>
		<url>https://github.com/objectos/incubator/tree/${project.scm.tag}</url>
		<tag>master</tag>
	</scm>

	<organization>
		<name>Objectos Software LTDA</name>
		<url>https://www.objectos.com.br/</url>
	</organization>

	<developers>
		<developer>
			<id>objectos</id>
			<name>Objectos Software LTDA</name>
			<email>opensource@objectos.com.br</email>
			<organization>Objectos Software LTDA</organization>
			<organizationUrl>https://www.objectos.com.br/</organizationUrl>
		</developer>
	</developers>

	<profiles>

		<profile>
			<id>java17</id>
			<activation>
				<activeByDefault>true</activeByDefault>
			</activation>
			<properties>
				<jdk>jdk17</jdk>
				<maven.toolchains.jdk-vendor>openjdk</maven.toolchains.jdk-vendor>
				<maven.toolchains.jdk-version>17</maven.toolchains.jdk-version>
				<project.classifier>java17</project.classifier>
			</properties>
			<build>
				<directory>${project.basedir}/target-java17</directory>
				<plugins>
					<plugin>
						<artifactId>maven-toolchains-plugin</artifactId>
					</plugin>
					<plugin>
						<groupId>org.codehaus.mojo</groupId>
						<artifactId>build-helper-maven-plugin</artifactId>
						<executions>
							<execution>
								<id>add-source</id>
								<phase>generate-sources</phase>
								<goals>
									<goal>add-source</goal>
								</goals>
								<configuration>
									<sources>
										<source>src/main/java6</source>
										<source>src/main/java7</source>
										<source>src/main/java8</source>
										<source>src/main/java11</source>
										<source>src/main/java17</source>
									</sources>
								</configuration>
							</execution>
							<execution>
								<id>add-test-source</id>
								<phase>generate-test-sources</phase>
								<goals>
									<goal>add-test-source</goal>
								</goals>
								<configuration>
									<sources>
										<source>src/test/java6</source>
										<source>src/test/java7</source>
										<source>src/test/java8</source>
										<source>src/test/java11</source>
										<source>src/test/java17</source>
									</sources>
								</configuration>
							</execution>
						</executions>
					</plugin>
					<plugin>
						<groupId>org.codehaus.mojo</groupId>
						<artifactId>flatten-maven-plugin</artifactId>
					</plugin>
				</plugins>
			</build>
		</profile>
























































































































































































































































	</profiles>

	<modules>
		<module>code/pom-java17.xml</module>
		<module>concurrent/pom-java17.xml</module>
		<module>core/pom-java17.xml</module>
		<module>css/pom-java17.xml</module>
		<module>docs/pom-java17.xml</module>
		<module>fs/pom-java17.xml</module>
		<module>git/pom-java17.xml</module>
		<module>html/pom-java17.xml</module>
		<module>http/pom-java17.xml</module>
		<module>latest/pom-java17.xml</module>
		<module>logger/pom-java17.xml</module>
		<module>mojo/pom-java17.xml</module>
		<module>mysql/pom-java17.xml</module>
		<module>office/pom-java17.xml</module>
		<module>smtp/pom-java17.xml</module>
		<module>ssg/pom-java17.xml</module>
	</modules>

	<build>
		<plugins>
			<plugin>
				<groupId>org.codehaus.mojo</groupId>
				<artifactId>build-helper-maven-plugin</artifactId>
				<executions>
					<execution>
						<id>add-source</id>
						<phase>generate-sources</phase>
						<goals>
							<goal>add-source</goal>
						</goals>
						<configuration>
							<sources>
								<source>src/main/java6</source>
								<source>src/main/java7</source>
								<source>src/main/java8</source>
								<source>src/main/java11</source>
								<source>src/main/java17</source>
							</sources>
						</configuration>
					</execution>
					<execution>
						<id>add-test-source</id>
						<phase>generate-test-sources</phase>
						<goals>
							<goal>add-test-source</goal>
						</goals>
						<configuration>
							<sources>
								<source>src/test/java6</source>
								<source>src/test/java7</source>
								<source>src/test/java8</source>
								<source>src/test/java11</source>
								<source>src/test/java17</source>
							</sources>
						</configuration>
					</execution>
				</executions>
			</plugin>
		</plugins>
	</build>

</project>